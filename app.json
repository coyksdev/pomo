        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.coyksdev.pomo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "492c3e77-0efa-4a01-b9ec-f0fab7294d7d"
      }
    }
  }
